{
  "canisters": {
    "bbdao": {
      "main": "bbdao/main.mo",
      "type": "motoko",
      "dependencies": ["webpage"]
    },
    "assets": {
      "dependencies": ["bbdao"],
      "interface": {
        "entrypoint": "dist/index.html"
      },
      "source": ["dist/"],
      "type": "assets"
    },
    "webpage": {
      "main": "webpage/main.mo",
      "type": "motoko"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
